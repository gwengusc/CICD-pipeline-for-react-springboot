{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json"]
    },
    "plugins": [
        "react"
    ],
    "ignorePatterns": [
      "*.d.ts"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
}
